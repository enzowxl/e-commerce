{
  "name": "e-commerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run --dir src/app/api/_use-cases",
    "test:watch": "vitest --dir src/app/api/_use-cases",
    "test:e2e": "vitest run --dir src/app/api/(http)",
    "test:watch:e2e": "vitest --dir src/app/api/(http)",
    "test:components": "vitest run --dir src/components --environment jsdom",
    "test:watch:components": "vitest --dir src/components --environment jsdom",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.378.0",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-dom": "^18",
    "stripe": "^15.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rocketseat/eslint-config": "^2.2.2",
    "@testing-library/react": "^15.0.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.5.3",
    "@vitest/ui": "^1.5.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "postcss": "^8",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^1.5.3"
  }
}
